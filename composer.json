{
    "name": "poluxGit/gdm",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/poluxGit/GDM",
    "description": "Generic Object Management",
    "keywords": ["personal", "poluxGit", "php"],
    "authors": [
        {
            "name": "polux_fr",
            "email": "poluxmsn@free.fr"
        }
    ],
    "require": {
        "php": ">=7.1",
				"pdepend/pdepend" : "@stable"
    },
    "require-dev": {
			"squizlabs/php_codesniffer": "~1.5.*",
			"phpmd/phpmd": "~2.1.*",
			"sebastian/phpcpd": "~2.0.*",
			"pdepend/pdepend": "~2.0.*",
			"phploc/phploc": "~2.0.*",
			"phpunit/phpunit": "~4.3.*",
			"theseer/phpdox": "~0.7.*"
    },
    "autoload": {
        "psr-4": {
            "GOM\\": "src/core"
        }
    },
		"config": {
			"optimize-autoloader": true
		}
}
